    "@wagmi/connectors": "^5.0.7",
    "connectkit": "^1.7.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-json-view": "^1.21.3",
    "react-router-dom": "^6.19.0",
