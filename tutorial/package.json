    "@wagmi/core": "^1.4.7",
    "@web3modal/wagmi": "^3.5.0",
    "axios": "^1.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-json-view": "^1.21.3",
    "react-router-dom": "^6.19.0",
    "viem": "^1.18.9",
